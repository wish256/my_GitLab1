pipeline
{
agent any
	stages
    {
	stage ("Build")
             {
            echo "Building phase"
	     }

            stage ("Deploy")
             {
            echo "Deploying phase"
	     }
	    stage ("Test")
             {
            echo "Testing phase"
	     }
stage ("Release")
             {
            echo "Release phase"
	     }
}

}
