pipeline
{
agent any
	stages
    {
	stage ("Build")
             {
	     steps{
            echo "Building phase"
	    }
	     }

            stage ("Deploy")
             {
	     steps{
            echo "Deploying phase"}
	     }
	    stage ("Test")
             {
	     steps{
            echo "Testing phase"}
	     }
stage ("Release")

             {
	     steps{
            echo "Release phase"}
	     }
}

}
